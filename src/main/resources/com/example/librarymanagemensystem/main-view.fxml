<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="420.0" prefWidth="379.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagemensystem.controllers.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button fx:id="view_books_button" mnemonicParsing="false" onAction="#handleViewBookButton" prefHeight="48.0" prefWidth="182.0" text="Manage Books" />
    <Button fx:id="manage_books_button" mnemonicParsing="false" prefHeight="48.0" onAction="#handleManagePatronsButton" prefWidth="171.0" text="Manage Patrons" />
    <Button mnemonicParsing="false" fx:id="issue_book_button" onAction="#handleIssueBookButton" prefHeight="40.0" prefWidth="179.0" text="Issue Book" />
    <Button mnemonicParsing="false" onAction="#handleReserveBookButton" prefHeight="39.0" prefWidth="184.0" text="Manage Reservations" />

    <Button mnemonicParsing="false" prefHeight="39.0" prefWidth="184.0" text="Return Book" />

    <Label fx:id="welcomeText" />
   <HBox prefHeight="100.0" prefWidth="200.0" />
   <Pane prefHeight="200.0" prefWidth="200.0" />
</VBox>
